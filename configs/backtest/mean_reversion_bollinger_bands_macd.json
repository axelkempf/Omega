{
  "symbol": "EURUSD",
  "timeframes": {
    "primary": "M30",
    "additional": []
},
  "mode": "candle",
  "enable_entry_logging": true,
  "initial_balance": 100000.0,
  "risk_per_trade": 100.0,

  "account_currency": "USD",
  "rates_static": {
    "EURUSD": 1.10,
    "EURAUD": 1.65,
    "EURJPY": 150.0,
    "EURGBP": 0.85,
    "GBPAUD": 1.90
  },

  "start_date": "2020-01-01",
  "end_date": "2025-07-20",
  
  "warmup_bars": 500,

  "cooldown_candles": 3,
  "cooldown_minutes": 45,
  "cooldown_minutes_trade": 5,

  "session_filter": {
    "fixed_times": [
      ["23:00", "19:00"]  
    ]
  },

  "strategy": {
    "module": "mean_reversion_bollinger_bands_plus_macd.backtest.backtest_strategy",
    "class": "MeanReversionBollingerBandsMACDBacktestStrategy",
    "parameters": { 
      "ema_length_4": 28,
      "bollinger_period": 20,
      "bollinger_std_factor": 1.5,
      "ratio_threshold": 0.45,
      "min_pip_mult": 5,
      "sl_mult": 1.6,
      "max_holding_minutes": 135,
      "breakeven_r_multiple": 1.0,
      "breakeven_buffer": 0.0001,
      "dynamic_tp_enabled": true
  }
  }
}