{
  "symbol": "EURUSD",
  "timeframes": {
    "primary": "M15",
    "additional": []
  },
  "mode": "candle",
  "enable_entry_logging": true,
  "initial_balance": 100000.0,
  "risk_per_trade": 100.0,

  "start_date": "2025-01-01",
  "end_date": "2025-07-20",
  
  "warmup_bars": 500,

  "cooldown_candles": 2,
  "cooldown_minutes": 15,

  "anchored_session_filter": {
    "windows": [
      { "anchor": "asia", "offset_window": [["-00:30", "01:00"]] },
      { "anchor": "london", "offset_window": [["-00:45", "01:30"]] },
      { "anchor": "new_york", "offset_window": [["-02:30", "00:15"]] }
    ]
  },

  "slippage": { 
    "fixed_pips": 0.2, 
    "random_pips": 0.3 
  },
  "fees": { 
    "per_million": 5.0 
  },

  "strategy": {
    "module": "trading_the_flow.backtest.backtest_strategy",
    "class": "TradingTheFlowBacktestStrategy",
    "symbol": "EURUSD",
    "parameters": {  
      "min_dist_pips": 3,
      "max_dist_pips": 20,
      "sl_mult": 1.1,
      "start": 30,
      "pip_threshhold": 0.00015,
      "max_holding_minutes": 120
    }
  }
}
