{
  "symbol": "EURUSD",
  "timeframes": {
    "primary": "H1",
    "additional": ["D1", "H4"]
  },
  "mode": "candle",
  "enable_entry_logging": true,
  "initial_balance": 100000.0,
  "risk_per_trade": 100.0,
  "account_currency": "USD",
  "rates": {
    "mode": "timeseries",
    "timeframe": "AUTO",
    "pairs": ["EURUSD"],
    "use_price": "close",
    "stale_limit_bars": 10,
    "strict": true
  },

  "start_date": "2020-01-01",
  "end_date": "2025-07-20",

  "warmup_bars": 500,

  "cooldown_candles": 2,
  "cooldown_minutes": 120,
  "cooldown_minutes_trade": 15,

  "session_filter": {
    "fixed_times": [
      ["23:00", "19:00"]
    ]
  },

  "timestamp_alignment": {
    "entry_timestamp_mode": "close",
    "additional_tfs_mode": "carry_forward",
    "stale_bar_limit_bars": 0,
    "use_previous_completed_higher_tf": true,
    "diagnostics": true,
    "higher_tf_timestamps_mode": {
      "D1": "close",
      "H4": "open"
    }
  },

  "strategy": {
    "module": "ema_rejection_trend_follow.backtest.backtest_strategy",
    "class": "EMARejectionTrendFollowBacktestStrategy",
    "parameters": {
      "ema_length_fast": 20,
      "ema_length_mid": 50,
      "ema_length_slow": 200,
      "ema_length_sl": 50,
      "choppiness_length": 14,
      "rsi_value_bearish": 50,
      "rsi_value_bullish": 50,
      "rsi_value_short": 30,
      "rsi_value_mid": 50,
      "rsi_value_long": 70,
      "rsi_length": 14,
      "choppiness_value_short": 38.2,
      "choppiness_value_long": 61.8,
      "atr_length": 14,
      "atr_mult": 0.8,
      "roc_length": 9,
      "dmi_length": 14,
      "tp_mult": 2,
      "sl_mult": 1.1,
      "body_mult": 2.5,
      "pip_threshhold": 0.00015,

      "max_holding_minutes": 300,
      "max_pending_minutes": 180,
      "breakeven_r_multiple": 1.0,
      "breakeven_buffer": 0.0001,

      "htf_tf": "D1",
      "htf_ema": 50,
      "htf_filter": "both",

      "extra_htf_tf": "H4",
      "extra_htf_ema": 50,
      "extra_htf_filter": "both"
    }
  }
}
